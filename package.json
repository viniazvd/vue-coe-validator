{
  "name": "vue-coe-validator",
  "version": "0.4.1",
  "description": "validator",
  "readmeFilename": "README.md",
  "keywords": [
    "validator",
    "validations",
    "validators",
    "validate",
    "input",
    "form"
  ],
  "cdn": "dist/vue-coe-validator.js",
  "main": "dist/vue-coe-validator.cjs.js",
  "unpkg": "dist/vue-coe-validator.js",
  "module": "dist/vue-coe-validator.es.js",
  "jsdelivr": "dist/vue-coe-validator.js",
  "umd:main": "dist/vue-coe-validator.js",
  "files": ["dist"],
  "author": {
    "name": "Vinicius Azevedo",
    "email": "viniazvd@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniazvd/vue-coe-validator"
  },
  "license": "MIT",
  "scripts": {
    "build": "bili src/index.js --formats cjs,es,umd --banner --js buble --moduleName VueCoeValidator",
    "test": "jest --verbose"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "rollup-plugin-vue": "^4.3.2",
    "bili": "^3.1.2",
    "jest": "^23.6.0",
    "vue-jest": "^3.0.0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "@vue/test-utils": "^1.0.0-beta.25"
  },
  "jest": {
    "testMatch": ["**/tests/**/*.js"],
    "moduleFileExtensions": ["js", "json", "vue"],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {  "^@/(.*)$": "<rootDir>/src/$1" }
  }
}
